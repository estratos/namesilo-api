{
  "name": "estratos/namesilo-api",
  "type": "symfony-bundle",
  "description": "Namesilo REST API Client for Symfony Framework",
  "keywords": ["php", "symfony", "namesilo", "domain registrar"],
  "license": "MIT",
  "authors": [
    {
      "name": "Julio Rodriguez",
      "email": "julio.stratos@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Estratos\\NamesiloApi\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      
    }
  },
  "require-dev": {
    "symfony/http-kernel": "^4.0"   
    
  },
  "require": {
    "php": ">=8.1",
    "symfony/config": "^7.1",
    "symfony/dependency-injection": "^7.1",
    "symfony/yaml": "^7.1"
    
  },

  "repositories": [
    {
        "type": "git",
        "url": "https://github.com/estratos/namesilo-api.git"
    }],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
